{
  "name": "monaco-unocss",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.14.3",
  "description": "UnoCSS integration for Monaco editor",
  "author": "Ilyal <ilyaliao324@gamil.com>",
  "license": "MIT",
  "homepage": "https://github.com/ilyaliao/monaco-unocss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilyaliao/monaco-unocss.git"
  },
  "bugs": "https://github.com/ilyaliao/monaco-unocss/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./unocss.worker": {
      "types": "./dist/unocss.worker.d.ts",
      "import": "./dist/unocss.worker.mjs",
      "require": "./dist/unocss.worker.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "play": "nr -C examples/vite dev",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "monaco-editor": ">=0.36"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0",
    "@unocss/autocomplete": "^65.4.0",
    "@unocss/core": "^65.4.0",
    "monaco-languageserver-types": "^0.4.0",
    "monaco-marker-data-provider": "^1.2.4",
    "monaco-types": "^0.1.0",
    "monaco-worker-manager": "^2.0.1",
    "vscode-languageserver-protocol": "^3.17.5",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@antfu/ni": "^0.23.1",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.10.1",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.14.3",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vite": "catalog:",
    "vitest": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
